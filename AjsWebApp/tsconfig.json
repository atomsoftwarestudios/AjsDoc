{
  "compileOnSave": false,

  "compilerOptions": {
    "outDir": "./buildtools/dist/",
    "allowJs": false,
    "module": "commonjs",
    "target": "es5",
    "lib": [ "es2015.promise", "es5" ],
    "noImplicitAny": false,
    "sourceMap": false,
    "removeComments": false
